<functions>
    <function name="seismicProcess">
		<srcLib>mylib</srcLib>
        <sourceDataType>8bits</sourceDataType>
        <destDataType>8bits</destDataType>
		<params>
			<param name="numPixelsString" type="int">
				<value>15</value>
				<lower>0</lower>
				<upper>50</upper>
			</param>
		</params>
    </function>
    <function name="hEdges">
		<srcLib>mylib</srcLib>
        <sourceDataType>8bits</sourceDataType>
        <destDataType>RGB</destDataType>
		<params>
			<param name="Threshold" type="int">
				<value>30</value>
				<lower>0</lower>
				<upper>255</upper>
			</param>
			<param name="Delta" type="int">
				<value>2</value>
				<lower>1</lower>
				<upper>15</upper>
			</param>
		</params>
    </function>
</functions>
